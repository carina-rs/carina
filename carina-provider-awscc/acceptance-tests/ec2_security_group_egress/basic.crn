# EC2 Security Group Egress - Basic test
#
# Tests: cidr_ip, from_port, to_port, ip_protocol, description

provider awscc {
  region = awscc.Region.ap_northeast_1
}

let vpc = awscc.ec2_vpc {
  cidr_block = "10.0.0.0/16"
}

let sg = awscc.ec2_security_group {
  vpc_id            = vpc.vpc_id
  group_description = "SG for egress rule test"
}

let egress = awscc.ec2_security_group_egress {
  group_id    = sg.group_id
  description = "Allow all outbound"
  ip_protocol = all
  cidr_ip     = "0.0.0.0/0"
}
