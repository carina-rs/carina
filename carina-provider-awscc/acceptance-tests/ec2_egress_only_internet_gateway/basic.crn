# EC2 Egress Only Internet Gateway - Basic test
#
# Tests: vpc_id, tags

provider awscc {
  region = aws.Region.ap_northeast_1
}

let vpc = awscc.ec2_vpc {
  cidr_block = "10.0.0.0/16"
}

let eoigw = awscc.ec2_egress_only_internet_gateway {
  vpc_id = vpc.vpc_id

  tags = {
    Environment = "acceptance-test"
  }
}
